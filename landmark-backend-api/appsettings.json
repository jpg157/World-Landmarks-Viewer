{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "localhost",
  "ConnectionStrings": {
    "Default": "Host=localhost; Port=5432; Database=app; Username=landmark_app; Password=${POSTGRESS_PASSWORD};"
  },
  "Auth0": {
    "Domain": "<auth0-tenant-domain>.auth0.com",
    "Audience": "https://<backend-api-identifier-set-in-auth0>"
  }
}
